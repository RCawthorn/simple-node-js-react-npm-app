pipeline {
    agent any
  
  stages {

		 
    stage(Build) {
	steps {
		echo "Build successful"
	           }
	 }



	stage (Test) {
	   steps {
		   echo "Test has passed"
		
	          }
			}
          
		stage(Deploy) {
            steps {
				sh echo "Deployment has been deployed"
            }
        }


		}
	   }   
